#!/usr/bin/env bash

cat << EOF
FoamFile
{
    version         2;
    format          ascii;
    class           volScalarField;
    object          k;
}

flowVelocity    ( 20 0 0 );

pressure        0;

turbulentKE     0.24;

turbulentOmega  1.78;

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 0.24;

boundaryField
{
    cyclic
    {
        type            cyclic;
    }
    processor
    {
        type            processor;
        value           uniform 0.24;
    }
    symmetry
    {
        type            symmetry;
    }
    motorBikeGroup
    {
        type            kqRWallFunction;
        value           uniform 0.24;
    }
    upperWall
    {
        type            slip;
    }
    frontAndBack
    {
        type            slip;
    }
}
Some more data may exist here:
{
  keyword
  {
    type test;
    inlet
    {
      type test2;
    }
  }
}
EOF
